@model (JenisAkun JenisAkun, string KodeJenisAkun)

<li>@Model.KodeJenisAkun @Model.JenisAkun.Uraian</li>

<ul>
    @foreach (var akun in Model.JenisAkun.DaftarAkun.Where(a => a.KelompokAkun is null && a.GolonganAkun is null))
    {
        <partial name="_AkunPartial" model="(akun, Model.KodeJenisAkun)" />
    }
</ul>

<ul>
    @foreach (var kelompokAkun in Model.JenisAkun.DaftarKelompokAkun)
    {
        <partial name="_KelompokAkunPartial" model="(kelompokAkun, Model.KodeJenisAkun)" />
    }
</ul>